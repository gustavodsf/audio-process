{   
    "pre_process":{
        "speech_path": "../data/speechs/",
        "music_path": "../data/music/",
        "segment_duration": 3,
        "sample_rate": 22050,
        "num_mfcc": 13,
        "hop_length": 512,
        "n_fft": 2048,
        "output": "processed_data.json",
